<div id="loading-wrapper">
    <div class="loading-bar label-center" style="width:100%;height:50%" , data-precision="0.01"
        data-stroke="data:ldbar/res,gradient(0,1,#9df,#9fd,#df9,#fd9)" , data-path="
    M90.5,23.2c0-12.5-10.2-22.7-22.7-22.7
    c-13.6,0-20.9,8.6-22.3,13.8C44.3,8.9,
    35.1,0.5,23.2,0.5C10.7,0.5,0.5,10.7,
    0.5,23.2c0,22.2,36.5,45.3,45,55.9
    C53.5,67.3,90.5,46.3,90.5,23.2z"></div>
</div>
<script>
    let bar = new ldBar(".loading-bar", {
        value: 0
    });
    let value = 0;
    let times = setInterval(() => {
        bar.set(value)
        value += 10
    }, 10)
    document.onreadystatechange = function () {
        if (document.readyState == 'complete') {
            console.log("complete")
            clearInterval(times)
            document.getElementById('loading-wrapper').style.display = 'none';
        }
    };
</script>